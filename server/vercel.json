{
  "version": 2,
  "routes": [
    { "src": "^/$", "dest": "/index.js" },
    { "src": "^/(.*)$", "dest": "/index.js" },
    {
      "src": "^/(auth|users|products|categories|orders|settings|contact)(.*)$",
      "dest": "/index.js"
    },
    { "src": "^/uploads/(.*)$", "dest": "/index.js" }
  ]
}
